# AI Agent Workspace Environment Variables
# Copy this file to .env and fill in your values

# Shopify Configuration
SHOPIFY_STORE_URL=https://nookleaf-dev.myshopify.com/

# GitHub Configuration
GITHUB_TOKEN=your_github_personal_access_token_here

# AI Service API Keys (Optional)
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key

# Database Configuration (for optional PostgreSQL)
POSTGRES_PASSWORD=secure_password_here

# Claude Flow Configuration
CLAUDE_FLOW_AUTO_COMMIT=false
CLAUDE_FLOW_AUTO_PUSH=false
CLAUDE_FLOW_HOOKS_ENABLED=true
CLAUDE_FLOW_TELEMETRY_ENABLED=true
CLAUDE_FLOW_REMOTE_EXECUTION=true
CLAUDE_FLOW_GITHUB_INTEGRATION=true

# Container Configuration
NODE_ENV=development
TZ=Europe/Berlin

# Playwright Configuration - Use Firefox as default
PLAYWRIGHT_BROWSER=firefox
PLAYWRIGHT_DEFAULT_BROWSER=firefox

# Resource Limits (optional overrides)
# DOCKER_CPU_LIMIT=4
# DOCKER_MEMORY_LIMIT=8G

# Additional API Keys (as needed)
# LANGCHAIN_API_KEY=your_langchain_api_key
# HUGGINGFACE_TOKEN=your_huggingface_token
# PINECONE_API_KEY=your_pinecone_api_key
# REDIS_URL=redis://redis:6379

# Security Settings
# ENABLE_SECURITY_SCANNING=true
# ENABLE_DEPENDENCY_CHECK=true